{
  "name": "expressboilerplate",
  "version": "2.0.0",
  "description": "Quick start for express app.",
  "main": "app.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src",
    "start": "node index.js",
    "dev": "nodemon --exec babel-node --presets latest ./src/bin/www",
    "build": "rm -rf ./lib && node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "prepublish": "npm run build",
    "test": "./node_modules/.bin/ava test.js --verbose"
  },
  "keywords": [
    "quick",
    "start",
    "node",
    "express",
    "application"
  ],
  "author": "Damir Mustafin",
  "license": "MIT",
  "dependencies": {
    "ava": "^0.18.2",
    "axios": "^0.15.3",
    "body-parser": "^1.13.3",
    "cheerio": "^0.22.0",
    "cookie-parser": "^1.3.5",
    "debug": "^2.2.0",
    "express": "^4.12.3",
    "express-session": "^1.14.0",
    "lusca": "^1.4.1",
    "morgan": "^1.6.1",
    "pug": "^2.0.0-beta6",
    "serve-favicon": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frenchbread/express-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/frenchbread/express-boilerplate/issues"
  },
  "homepage": "https://github.com/frenchbread/express-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-latest": "^6.22.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^2.2.0"
  }
}
